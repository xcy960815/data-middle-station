{
  "name": "data-middle-station",
  "version": "0.0.1",
  "description": "使用nuxt3 + element-plus + pinia + mysql + pm2 搭建的数据中台",
  "scripts": {
    "build": "nuxt build --dotenv env/.env.daily",
    "build:pre": "nuxt build --dotenv env/.env.pre",
    "build:prod": "nuxt build --dotenv env/.env.prod",
    "dev": "nuxt dev --dotenv env/.env.daily",
    "dev:pre": "nuxt dev --dotenv env/.env.pre",
    "dev:prod": "nuxt dev --dotenv env/.env.prod",
    "postinstall": "nuxt prepare",
    "prepare": "husky install",
    "prestart": "pnpm build",
    "start:daily": "pm2 start ecosystem.config.js --env daily",
    "prestart:pre": "pnpm build:pre",
    "start:pre": "pm2 start ecosystem.config.js --env pre",
    "prestart:prod": "pnpm build:prod",
    "start:prod": "pm2 start ecosystem.config.js --env prod",
    "stop": "pm2 stop ecosystem.config.js",
    "poststop": "pnpm delete",
    "delete": "pm2 delete ecosystem.config.js",
    "restart": "pm2 restart ecosystem.config.js --env daily",
    "restart:pre": "pm2 restart ecosystem.config.js --env pre",
    "restart:prod": "pm2 restart ecosystem.config.js --env prod",
    "changelog": "conventional-changelog -c changelog.config.js -i CHANGELOG.md -s --commit-path . --date-format YYYY-MM-DD --output-unreleased",
    "changelog:first": "conventional-changelog -c changelog.config.js -i CHANGELOG.md -s -r 0 --date-format YYYY-MM-DD --output-unreleased",
    "changelog:version": "conventional-changelog -c changelog.config.js -i CHANGELOG.md -s -r 0 --release-count 0 --date-format YYYY-MM-DD --output-unreleased",
    "format": "prettier --write \"**/*.{js,ts,vue,json,css,scss,less,html,md}\"",
    "format:check": "prettier --check \"**/*.{js,ts,vue,json,css,scss,less,html,md}\"",
    "lint": "eslint --config eslint.config.js .",
    "lint:fix": "eslint --config eslint.config.js . --fix"
  },
  "overrides": {
    "vue": "latest"
  },
  "pnpm": {
    "overrides": {
      "undici": "6.21.0"
    }
  },
  "optionalDependencies": {
    "@rollup/rollup-darwin-x64": "4.12.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.8.1",
    "@commitlint/config-conventional": "^17.8.1",
    "@element-plus/nuxt": "^1.1.4",
    "@eslint/js": "^9.28.0",
    "@nuxt/devtools": "latest",
    "@types/node": "^20.19.0",
    "@types/node-cron": "^3.0.11",
    "@types/node-schedule": "^2.1.8",
    "@types/nodemailer": "^7.0.1",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "commitlint": "^17.8.1",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^9.28.0",
    "eslint-plugin-vue": "^10.1.0",
    "globals": "^16.2.0",
    "husky": "^8.0.3",
    "less": "^4.3.0",
    "less-loader": "^11.1.4",
    "lint-staged": "^15.5.2",
    "pm2": "^6.0.6",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "pretty-quick": "^4.1.1",
    "sass": "^1.89.0",
    "tailwindcss": "^3.4.17",
    "vue-eslint-parser": "^10.0.1"
  },
  "dependencies": {
    "@icon-park/vue-next": "^1.4.2",
    "@pinia/nuxt": "^0.5.1",
    "@popperjs/core": "npm:@sxzz/popperjs-es@^2.11.7",
    "@types/boxen": "^3.0.5",
    "@types/figlet": "^1.7.0",
    "@types/gradient-string": "^1.1.6",
    "@types/jsonwebtoken": "^9.0.10",
    "boxen": "^8.0.1",
    "canvas": "^3.2.0",
    "chalk": "^5.4.1",
    "common-watermark": "^0.0.1",
    "dayjs": "^1.11.13",
    "echarts": "^6.0.0",
    "element-plus": "^2.11.1",
    "figlet": "^1.8.2",
    "gradient-string": "^3.0.0",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2",
    "konva": "^9.3.22",
    "monaco-editor": "^0.52.2",
    "mysql2": "^3.14.1",
    "node-cron": "^3.0.3",
    "node-schedule": "^2.1.1",
    "nodemailer": "^7.0.6",
    "nuxt": "3.17.5",
    "pinia": "^2.1.7",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "vue-server-renderer": "^2.7.16",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^4.7.1",
    "xcytest-package": "^1.0.3",
    "xlsx": "^0.18.5"
  },
  "volta": {
    "node": "20.19.0"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "prettier --write"
    ],
    "*.{json,css,scss,less,html,md}": [
      "prettier --write"
    ]
  }
}
